{
  "workbench.colorTheme": "Night Owl Light (No Italics)",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontWeight": "600",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "macros": {
    "addSemicolon": [
      "cursorEnd",
      {
        "command": "type",
        "args": {
          "text": ";"
        }
      }
    ],
    "addCurlyBrackets": [
      "cursorEnd",
      {
        "command": "type",
        "args": {
          "text": " {"
        }
      }
    ],
    "addComma": [
      "cursorEnd",
      {
        "command": "type",
        "args": {
          "text": ","
        }
      }
    ],
    "addEqual": [
      "cursorEnd",
      {
        "command": "type",
        "args": {
          "text": " = "
        }
      }
    ]
  },
  "workbench.iconTheme": "mosmmy-icons-vscode",
  "editor.tabSize": 2,
  "[javascriptreact]": {
